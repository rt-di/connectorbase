<Table
	items="{path:'/topics', templateShareable:false}">
	<columns>
		<Column>
			<Label text="Topic Name" />
		</Column>
	</columns>
	<items>
		<ColumnListItem>
			<cells>
				<HBox alignItems="Center">
					<core:Icon src="sap-icon://detailed-view" />
					<Link text="{topicname}" href="{path : 'topicname', formatter: '.topicformatter'}" />
				</HBox>				
			</cells>
		</ColumnListItem>
	</items>
</Table>
